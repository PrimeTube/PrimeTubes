<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">
	<LinearLayout
		android:id="@+id/actionbar_linear"
		android:layout_width="match_parent"
		android:layout_height="50dp"
		android:layout_margin="5dp"
		android:background="#E0E0E0"
		android:gravity="center_horizontal|center_vertical"
		android:orientation="horizontal">
		<ImageView
			android:id="@+id/imageview7"
			android:layout_width="50dp"
			android:layout_height="50dp"
			android:src="@drawable/download"
			android:scaleType="fitCenter" />
		<LinearLayout
			android:id="@+id/linear66"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:background="#FFFFFF"
			android:gravity="right|center_vertical"
			android:orientation="horizontal">
			<ImageView
				android:id="@+id/imageview2"
				android:layout_width="60dp"
				android:layout_height="wrap_content"
				android:src="@drawable/searching"
				android:scaleType="fitCenter" />
		</LinearLayout>
	</LinearLayout>
	<LinearLayout
		android:id="@+id/main_linear"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:background="#FFFFFF"
		android:orientation="horizontal"
		android:layout_weight="1">
		<LinearLayout
			android:id="@+id/video_linear"
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:background="@android:color/transparent"
			android:orientation="vertical">
			<ProgressBar
				android:id="@+id/progressbar6"
				android:layout_width="match_parent"
				android:layout_height="5dp"
				android:indeterminate="true"
				style="?android:progressBarStyleHorizontal"
				android:layout_gravity="center_horizontal|center_vertical" />
			<ListView
				android:id="@+id/listview1"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_margin="5dp"
				android:dividerHeight="0dp"
				android:divider="@null"
				android:choiceMode="none"
				tools:listitem="@layout/ad_screen_photo" />
		</LinearLayout>
		<LinearLayout
			android:id="@+id/upload_linear"
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:padding="5dp"
			android:gravity="center_horizontal|center_vertical"
			android:orientation="vertical">
			<ScrollView
				android:id="@+id/vscroll2"
				android:layout_width="match_parent"
				android:layout_height="wrap_content">
				<LinearLayout
					android:id="@+id/linear64"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:orientation="horizontal">
					<LinearLayout
						android:id="@+id/upload2linear"
						android:layout_width="match_parent"
						android:layout_height="match_parent"
						android:gravity="center_horizontal|center_vertical"
						android:orientation="vertical">
						<LinearLayout
							android:id="@+id/linear60"
							android:layout_width="match_parent"
							android:layout_height="200dp"
							android:gravity="center_horizontal|center_vertical"
							android:orientation="vertical">
							<LinearLayout
								android:id="@+id/linear59"
								android:layout_width="match_parent"
								android:layout_height="wrap_content"
								android:gravity="center_horizontal|center_vertical"
								android:orientation="horizontal">
								<Button
									android:id="@+id/pick_image"
									android:layout_width="130dp"
									android:layout_height="40dp"
									android:layout_margin="8dp"
									android:background="#9C27B0"
									android:text="Video Thumbnail"
									android:textSize="12sp"
									android:textStyle="bold"
									android:textColor="#FFFFFF" />
								<Button
									android:id="@+id/pick_videos"
									android:layout_width="130dp"
									android:layout_height="40dp"
									android:layout_margin="8dp"
									android:background="#9C27B0"
									android:text="Video select"
									android:textSize="12sp"
									android:textStyle="bold"
									android:textColor="#FFFFFF" />
							</LinearLayout>
							<LinearLayout
								android:id="@+id/edittext_linear"
								android:layout_width="match_parent"
								android:layout_height="wrap_content"
								android:background="#FFFFFF"
								android:gravity="center_horizontal|center_vertical"
								android:orientation="vertical">
								<LinearLayout
									android:id="@+id/linear70"
									android:layout_width="match_parent"
									android:layout_height="wrap_content"
									android:orientation="horizontal">
									<EditText
										android:id="@+id/edittext1"
										android:layout_width="match_parent"
										android:layout_height="wrap_content"
										android:padding="8dp"
										android:background="@android:color/transparent"
										android:textSize="12sp"
										android:textColor="#000000"
										android:hint="your name"
										android:textColorHint="#607D8B" />
								</LinearLayout>
								<EditText
									android:id="@+id/edittext_title"
									android:layout_width="match_parent"
									android:layout_height="wrap_content"
									android:layout_margin="8dp"
									android:padding="8dp"
									android:background="#CE93D8"
									android:textSize="12sp"
									android:textColor="#000000"
									android:hint="What's your video title"
									android:textColorHint="#607D8B" />
								<LinearLayout
									android:id="@+id/linear61"
									android:layout_width="match_parent"
									android:layout_height="10dp"
									android:padding="8dp"
									android:background="#FFFFFF"
									android:orientation="horizontal" />
								<EditText
									android:id="@+id/edittext_description"
									android:layout_width="match_parent"
									android:layout_height="50dp"
									android:layout_margin="8dp"
									android:padding="8dp"
									android:background="#CE93D8"
									android:textSize="12sp"
									android:textColor="#000000"
									android:hint="Enter Your  Description"
									android:textColorHint="#607D8B"
									android:layout_gravity="center_horizontal|center_vertical" />
							</LinearLayout>
						</LinearLayout>
						<LinearLayout
							android:id="@+id/linear62"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:gravity="center_horizontal|center_vertical"
							android:orientation="vertical">
							<LinearLayout
								android:id="@+id/linear72"
								android:layout_width="match_parent"
								android:layout_height="wrap_content"
								android:padding="8dp"
								android:orientation="horizontal">
								<Button
									android:id="@+id/upload"
									android:layout_width="match_parent"
									android:layout_height="40dp"
									android:layout_margin="8dp"
									android:background="#9C27B0"
									android:text="UPLOAD YOUR VIDEO"
									android:textSize="12sp"
									android:textStyle="bold"
									android:textColor="#FFFFFF" />
							</LinearLayout>
							<LinearLayout
								android:id="@+id/linear73"
								android:layout_width="match_parent"
								android:layout_height="wrap_content"
								android:padding="8dp"
								android:orientation="horizontal">
								<CheckBox
									android:id="@+id/checkbox2"
									android:layout_width="wrap_content"
									android:layout_height="wrap_content"
									android:padding="8dp"
									android:text="Accept SKETCHit video uploading terms and condition"
									android:textSize="12sp"
									android:textColor="#000000" />
							</LinearLayout>
							<LinearLayout
								android:id="@+id/videopic_linear"
								android:layout_width="match_parent"
								android:layout_height="250dp"
								android:padding="5dp"
								android:gravity="center_horizontal|center_vertical"
								android:orientation="horizontal">
								<LinearLayout
									android:id="@+id/linear1"
									android:layout_width="wrap_content"
									android:layout_height="wrap_content"
									android:padding="2dp"
									android:background="#000000"
									android:gravity="center_horizontal|center_vertical"
									android:orientation="horizontal" />
							</LinearLayout>
							<LinearLayout
								android:id="@+id/linear69"
								android:layout_width="match_parent"
								android:layout_height="200dp"
								android:padding="8dp"
								android:orientation="horizontal">
								<ImageView
									android:id="@+id/image_add_view"
									android:layout_width="match_parent"
									android:layout_height="match_parent"
									android:src="@drawable/lll"
									android:scaleType="fitCenter" />
							</LinearLayout>
							<TextView
								android:id="@+id/video_title_textview"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:padding="8dp"
								android:text="video title"
								android:textSize="10sp"
								android:textColor="#000000" />
						</LinearLayout>
					</LinearLayout>
				</LinearLayout>
			</ScrollView>
			<TextView
				android:id="@+id/image_title_textview"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:padding="8dp"
				android:text="image title"
				android:textSize="10sp"
				android:textColor="#000000" />
		</LinearLayout>
	</LinearLayout>
	<LinearLayout
		android:id="@+id/progreesbar_linear"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:orientation="vertical">
		<ProgressBar
			android:id="@+id/progressbar3"
			android:layout_width="match_parent"
			android:layout_height="5dp"
			android:indeterminate="true"
			style="?android:progressBarStyleHorizontal"
			android:layout_gravity="center_horizontal|center_vertical" />
		<LinearLayout
			android:id="@+id/up_dw_porgres_linear"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:background="#ECEFF1"
			android:gravity="center_horizontal|center_vertical"
			android:orientation="horizontal">
			<TextView
				android:id="@+id/loading_number"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:text="upload"
				android:textSize="11sp"
				android:textStyle="bold"
				android:textColor="#000000" />
			<ImageView
				android:id="@+id/image_add_ok"
				android:layout_width="30dp"
				android:layout_height="30dp"
				android:src="@drawable/check"
				android:scaleType="fitCenter" />
		</LinearLayout>
	</LinearLayout>
	<LinearLayout
		android:id="@+id/bottombar_linear"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_margin="5dp"
		android:background="#FFFFFF"
		android:gravity="center_horizontal|center_vertical"
		android:orientation="horizontal">
		<ImageView
			android:id="@+id/imageview3"
			android:layout_width="30dp"
			android:layout_height="30dp"
			android:src="@drawable/ico_1"
			android:scaleType="fitCenter"
			android:layout_weight="1" />
		<ImageView
			android:id="@+id/imageview1"
			android:layout_width="50dp"
			android:layout_height="50dp"
			android:background="@drawable/icon3"
			android:src="@drawable/ic_add_black"
			android:scaleType="fitCenter" />
		<ImageView
			android:id="@+id/imageview5"
			android:layout_width="30dp"
			android:layout_height="30dp"
			android:src="@drawable/ic_person_black"
			android:scaleType="fitCenter"
			android:layout_weight="1" />
	</LinearLayout>
</LinearLayout>
